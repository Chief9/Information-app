style.
	nav	ul{
	list-style-type: none;
	margin: 0;
	padding: 0;
	overflow: hidden;
	background-color: #333;
	}

	nav	p	{
	float: right;
	color: white;
	} 

	nav	li {
	float: left;
	}

	nav	li a {
	display: block;
	color: white;
	text-align: center;
	padding: 14px 16px;
	text-decoration: none;
	}

	nav	a:hover:not(.active) {
	background-color: #111;
	}

	nav	.active {
	background-color:#4CAF50;
	}